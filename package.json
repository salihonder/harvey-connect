		"test": "jest"
	},
	"dependencies": {
		"lodash": "^4.17.13",
		"react": "16.0.0-alpha.12",
		"react-native": "0.48.4",
		"react-native-communications": "^2.2.1",
		"react-native-fabric": "^0.5.0",
		"react-native-localization": "^0.1.32",
		"react-native-mail": "^3.0.4",
		"react-native-maps": "^0.16.4",
		"react-native-page-control": "^1.1.0",
		"react-native-vector-icons": "^4.3.0",
		"react-navigation": "^1.0.0-beta.13",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2"
	},
	"devDependencies": {
		"@types/jest": "^21.1.1",
		"@types/lodash": "^4.14.74",
		"@types/react": "^16.0.7",
		"@types/react-native": "^0.48.9",
		"@types/react-native-communications": "^2.2.0",
		"@types/react-redux": "^5.0.6",
		"@types/react-test-renderer": "^15.5.4",
		"babel-jest": "^20.0.1",
		"babel-preset-react-native": "4.0.0",
		"jest": "^21.1.0",
		"react-native-typescript-transformer": "^1.1.4",
		"react-test-renderer": "^16.0.0",
		"ts-jest": "^21.0.1",
		"tslint": "^5.7.0",
		"typescript": "2.5.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
			"\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"testPathIgnorePatterns": [
			"\\.snap$",
			"<rootDir>/node_modules/",
			"<rootDir>/lib/"
		],
		"cacheDirectory": ".jest/cache"
	}
}
